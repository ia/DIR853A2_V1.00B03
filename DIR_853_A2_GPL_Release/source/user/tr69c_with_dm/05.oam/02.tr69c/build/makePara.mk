

ifndef PRC_CROSS_COMPILE
PRC_CROSS_COMPILE := /opt/buildroot-gcc463/usr/bin/mipsel-linux-
endif
CC := $(PRC_CROSS_COMPILE)gcc
AR := $(PRC_CROSS_COMPILE)ar
link := $(PRC_CROSS_COMPILE)gcc
STRIP := $(PRC_CROSS_COMPILE)strip

CURRENT_PATH := $(shell pwd)
LIB_PATH := $(CURRENT_PATH)/libs/
OBJ_PATH := $(CURRENT_PATH)/objs/

TIME = `date`
TR69_BUILDS += -D BUILD_TIME="\"$(TIME)\""

CFLAGS := -g -Wall -Wshadow -Wcast-qual
CFLAGS += -DDESKTOP_LINUX -DCMS_LOG3 -DMDM_SHARED_MEM -DDMP_BASELINE_1 -g -D_DEBUG
CFLAGS += -DUSE_LIBTRACER -DUSE_LIBCARES -DSUPPORT_DM_PURE181 -DUSE_OAM_ADAPTOR
CFLAGS += -DDMP_X_BROADCOM_COM_DEBUG_1 -DDMP_X_BROADCOM_COM_SECURITY_1 -DDMP_DEVICE2_BASELINE_1 -DDMP_DEVICE2_TIME_1
CFLAGS += -DSUPPORT_IPV6
CFLAGS += -DUSE_SSL -DORIGINAL_OPENSSL -DDEBUGSSL

USE_OPENSSL := .
